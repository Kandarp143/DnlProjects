	<queryString>
		<![CDATA[SELECT cba_bill_mst.status,cba_bill_mst.bill_usr_no as bill_id,cba_bill_mst.bill_no, cba_bill_mst.po_no, cba_bill_mst.bill_date,cba_wo_mst.po_title,
       cba_bill_mst.cr_date, DECODE (cba_bill_mst.ou, '123', 'Nandasari') AS ou, cba_user_mst.full_name,
       cba_bill_mst.status, cba_bill_mst.val, cba_bill_mst.tax_val,
       cba_bill_mst.total_val, cba_bill_mst.bill_desc, po_vendors.vendor_name,
       po_vendor_sites_all.vendor_site_code AS site, cba_wo_mst.cur,cba_wo_mst.ret_amt,
                          po_vendor_sites_all.address_line1
                || po_vendor_sites_all.address_line2
                || ' '
                || po_vendor_sites_all.city
                || ', '
                || po_vendor_sites_all.state
                || ',  '
                || DECODE (po_vendor_sites_all.country, 'IN', 'India')
                || '.' as Address
  FROM cba_bill_mst, cba_wo_mst, po_vendors, cba_user_mst,po_vendor_sites_all
 WHERE (    (cba_wo_mst.po_no = cba_bill_mst.po_no)
        AND (cba_bill_mst.user_id = cba_user_mst.user_id)
           AND (cba_wo_mst.site = po_vendor_sites_all.vendor_site_id)
                 AND (po_vendors.vendor_id = po_vendor_sites_all.vendor_id)
        AND(cba_bill_mst.bill_no= $P{bill})
       )]]>
	</queryString>
