		</parameter>
		<queryString>
			<![CDATA[/* Formatted on 2015/04/21 16:23 (Formatter Plus v4.8.8) */
SELECT rownum,cba_att_mst.bill_no, cba_att_mst.f_name, cba_user_mst.full_name,
       cba_att_mst.act_date
  FROM cba_att_mst, cba_user_mst
 WHERE ((cba_att_mst.user_id = cba_user_mst.user_id)
