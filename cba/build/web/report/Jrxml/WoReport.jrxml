	<subDataset name="workflow" uuid="65dfa934-21c8-49eb-ae4e-60ad2d2ef14b">
		<parameter name="pono" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT   cba_stg_mst.stg_name AS factivity,
         cba_stg_mst2.stg_name AS tactivity, cba_user_mst.full_name,
         DECODE (cba_act_mst.act_name,
                 'Promote', 'Approved',
                 'Create', 'Create',
                 'Reject', 'Reject',
                 'Update', 'Update',
                 'Demote', 'Demote'
                )act_name ,
         cba_tran_wo.act_date, cba_tran_wo.cmt, cba_user_mst2.full_name,
         DECODE (cba_tran_wo.is_comp,
                 NULL, 'Pending',
                 'Y', 'Completed',
                 'R', 'Rejected'
                ) status
    FROM cba_act_mst,
         cba_tran_wo,
         cba_user_mst,
         cba_wo_mst,
         cba_stg_mst,
         cba_stg_mst cba_stg_mst2,
         cba_user_mst cba_user_mst2
   WHERE (    (cba_act_mst.act_id = cba_tran_wo.act_id)
          AND (cba_wo_mst.po_no = cba_tran_wo.po_number)
          AND (cba_user_mst.user_id = cba_tran_wo.user_id)
          AND (cba_user_mst2.user_id = cba_tran_wo.nxt_uid)
          AND (cba_stg_mst.stg_id = cba_tran_wo.stg_c)
          AND (cba_stg_mst2.stg_id = cba_tran_wo.stg_n)
          AND (cba_tran_wo.po_number = $P{pono})
         )
ORDER BY cba_tran_wo.tran_id]]>
		</queryString>
		<field name="FACTIVITY" class="java.lang.String"/>
		<field name="TACTIVITY" class="java.lang.String"/>
		<field name="FULL_NAME" class="java.lang.String"/>
		<field name="ACT_NAME" class="java.lang.String"/>
		<field name="ACT_DATE" class="java.sql.Timestamp"/>
		<field name="CMT" class="java.lang.String"/>
		<field name="STATUS" class="java.lang.String"/>
	</subDataset>
	<subDataset name="attachment" uuid="91bc22b1-a770-4ad0-9bc4-62e1ccce4787">
		<parameter name="pono" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT rownum,cba_att_mst.f_path, cba_att_mst.f_name, cba_att_mst.po_number,
       cba_att_mst.bill_no, cba_user_mst.full_name, cba_att_mst.act_date
  FROM cba_user_mst, cba_att_mst
 WHERE (    (cba_user_mst.user_id = cba_att_mst.user_id)
        AND (cba_att_mst.att_type = 'ATT')
        AND (cba_att_mst.po_number = $P{pono})
       )]]>
		</queryString>
		<field name="ROWNUM" class="java.math.BigDecimal"/>
		<field name="F_PATH" class="java.lang.String"/>
		<field name="F_NAME" class="java.lang.String"/>
		<field name="PO_NUMBER" class="java.lang.String"/>
		<field name="BILL_NO" class="java.lang.String"/>
		<field name="FULL_NAME" class="java.lang.String"/>
		<field name="ACT_DATE" class="java.sql.Timestamp"/>
	</subDataset>
	<parameter name="pono" class="java.lang.String">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[/* Formatted on 2015/04/23 17:11 (Formatter Plus v4.8.8) */
SELECT DISTINCT cba_wo_mst.po_no, cba_wo_mst.cr_date, cba_wo_mst.exp_date,cba_wo_mst.fr_date,
                DECODE (cba_wo_mst.ou, '123', 'Nandasari') AS ou,
                cba_user_mst.full_name, po_vendors.vendor_name,
                po_vendor_sites_all.vendor_site_code AS site, cba_wo_mst.cur,
                cba_wo_mst.status, cba_wo_mst.TYPE, cba_wo_mst.wo_desc,
                cba_wo_mst.ret_amt, cba_wo_mst.val, cba_wo_mst.tax_val,
                cba_wo_mst.total_val, cba_wo_mst.po_title,
                   po_vendor_sites_all.address_line1
                || po_vendor_sites_all.address_line2
                || ' '
                || po_vendor_sites_all.city
                || ', '
                || po_vendor_sites_all.state
                || ',  '
                || DECODE (po_vendor_sites_all.country, 'IN', 'India')
                || '.' as Address
           FROM cba_wo_mst,
                cba_user_mst,
                cba_user_mst cba_user_mst2,
                po_vendors,
                po_vendor_sites_all
          WHERE (    (cba_wo_mst.user_id = cba_user_mst.user_id)
                 AND (cba_wo_mst.sup_id = po_vendors.vendor_number)
	      AND (cba_wo_mst.site = po_vendor_sites_all.vendor_site_id)
                 AND (po_vendors.vendor_id = po_vendor_sites_all.vendor_id)
                 AND (cba_wo_mst.po_no = $P{pono})
                )]]>
	</queryString>
