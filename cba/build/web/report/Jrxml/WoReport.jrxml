	<subDataset name="item" whenResourceMissingType="Empty" uuid="9efe0ca7-1e1c-4eac-87fa-7a48c9116b43">
		<parameter name="pono" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT cba_wo_item.line_no,rownum,cba_wo_item.item_id, mtl_system_items.description, cba_wo_item.uom,
       cba_wo_item.qty, cba_wo_item.rate, cba_wo_item.val, DECODE(cba_wo_item.cmt,'','-') as cmt,mtl_system_items.segment1 itm_no
  FROM cba_wo_item, mtl_system_items
 WHERE ((cba_wo_item.item_id = mtl_system_items.segment1)
 AND (mtl_system_items.organization_id = 0)
 AND (cba_wo_item.po_no = $P{pono})
 ) order by cba_wo_item.line_no]]>
		</queryString>
		<field name="LINE_NO" class="java.math.BigDecimal"/>
		<field name="ROWNUM" class="java.math.BigDecimal"/>
		<field name="ITEM_ID" class="java.lang.String"/>
		<field name="DESCRIPTION" class="java.lang.String"/>
		<field name="UOM" class="java.lang.String"/>
		<field name="QTY" class="java.lang.Double"/>
		<field name="RATE" class="java.lang.Double"/>
		<field name="VAL" class="java.lang.Double"/>
		<field name="CMT" class="java.lang.String"/>
		<field name="ITM_NO" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Tax" uuid="c0f070fe-6ed6-491c-826b-d401b792ac00">
		<parameter name="pono" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT rownum,ja_in_tax_codes.tax_id, ja_in_tax_codes.tax_name,cba_wo_mst.val,
       ja_in_tax_codes.tax_descr, cba_wo_tax.tax_val as tax_rate,to_number(cba_wo_mst.val)*(cba_wo_tax.tax_val/100) as tax_value
  FROM cba_wo_mst,cba_wo_tax, ja_in_tax_codes
 WHERE ((cba_wo_tax.tax_type = ja_in_tax_codes.tax_id)
 AND (cba_wo_mst.po_no = cba_wo_tax.po_no)
 AND (cba_wo_tax.po_no = $P{pono}))]]>
		</queryString>
		<field name="ROWNUM" class="java.math.BigDecimal"/>
		<field name="TAX_ID" class="java.math.BigDecimal"/>
		<field name="TAX_NAME" class="java.lang.String"/>
		<field name="VAL" class="java.lang.Double"/>
		<field name="TAX_DESCR" class="java.lang.String"/>
		<field name="TAX_RATE" class="java.lang.Double"/>
		<field name="TAX_VALUE" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="pay" uuid="45ee495a-58ec-472f-b9bb-f0c02131e5c8">
		<parameter name="pono" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT ap_terms.NAME,ap_terms.DESCRIPTION
  FROM ap_terms, cba_wo_term
 WHERE ((cba_wo_term.term_id = ap_terms.term_id)
 AND (cba_wo_term.po_no = $P{pono})
 )]]>
